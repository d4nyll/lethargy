{
  "name": "lethargy",
  "version": "1.0.8",
  "description": "Distinguish between scroll events initiated by the user, and those by inertial scrolling",
  "main": "lethargy.js",
  "scripts": {
    "test": "./node_modules/.bin/prova ./test/**/*.js -b -l phantom -q",
    "test-browser": "./node_modules/.bin/prova ./test/**/*.js -b -l chrome",
    "postinstall": "opencollective postinstall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d4nyll/lethargy.git"
  },
  "keywords": [
    "scroll",
    "wheel",
    "mouse",
    "mousewheel",
    "scrolljacking",
    "inertial",
    "trackpad",
    "magic",
    "mouse"
  ],
  "author": "Daniel Li <dan@danyll.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d4nyll/lethargy/issues"
  },
  "homepage": "https://github.com/d4nyll/lethargy",
  "devDependencies": {
    "jquery": "^3.4.1",
    "prova": "^3.0.0",
    "tiny-trigger": "scottcorgan/tiny-trigger"
  },
  "dependencies": {
    "opencollective": "^1.0.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/lethargy",
    "logo": "https://opencollective.com/lethargy/logo.txt"
  }
}